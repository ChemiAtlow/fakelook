{
    "name": "@fakelook/service_authentication",
    "version": "0.1.0",
    "description": "A authentication micro service",
    "author": "Chemi Atlow",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "license": "MIT",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc -b",
        "test": "echo \"Error: run tests from root\" && exit 1",
        "lint": "eslint src --ext ts",
        "execute": "nodemon -r dotenv/config --inspect ./dist/app.js",
        "prebuild": "rimraf \"./dist\"",
        "build:watch": "nodemon --watch src/ -e ts --exec \"npm run lint && npm run build && npm run execute\"",
        "start:dev": "npm run build:watch"
    },
    "dependencies": {
        "@fakelook/common": "^0.1.0",
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "google-auth-library": "^7.0.2",
        "helmet": "4.4.1",
        "jsonwebtoken": "^8.5.1",
        "morgan": "^1.10.0",
        "mysql2": "^2.2.5",
        "nodemailer": "^6.4.17",
        "sequelize": "^6.5.0",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/compression": "^1.7.0",
        "@types/cors": "^2.8.8",
        "@types/express": "^4.17.8",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/morgan": "^1.9.2",
        "@types/node": "^14.14.20",
        "@types/nodemailer": "^6.4.0",
        "@types/validator": "^13.1.3",
        "@typescript-eslint/eslint-plugin": "^4.13.0",
        "@typescript-eslint/parser": "^4.4.1",
        "dotenv": "^8.2.0",
        "eslint": "^7.9.0",
        "nodemon": "^2.0.4",
        "rimraf": "^3.0.2",
        "typescript": "^4.1.5"
    }
}
