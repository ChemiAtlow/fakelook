{
    "name": "@fakelook/service_identity",
    "version": "0.1.0",
    "description": "A identity micro service",
    "author": "Chemi Atlow",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "license": "MIT",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc -b",
        "test": "echo \"Error: run tests from root\" && exit 1",
        "lint": "eslint src --ext ts",
        "execute": "nodemon --inspect=4442 ./dist/app.js",
        "prebuild": "rimraf \"./dist\"",
        "build:watch": "nodemon --watch src/ -e ts --exec \"npm run lint && npm run build && npm run execute\"",
        "start:dev": "npm run build:watch"
    },
    "dependencies": {
        "@fakelook/common": "^0.1.0",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "helmet": "4.4.1",
        "morgan": "^1.10.0",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@types/compression": "^1.7.0",
        "@types/cors": "^2.8.8",
        "@types/express": "^4.17.8",
        "@types/morgan": "^1.9.2",
        "@types/node": "^14.14.20",
        "@typescript-eslint/eslint-plugin": "^4.13.0",
        "@typescript-eslint/parser": "^4.4.1",
        "eslint": "^7.9.0",
        "nodemon": "^2.0.4",
        "rimraf": "^3.0.2",
        "typescript": "^4.2.2"
    }
}
